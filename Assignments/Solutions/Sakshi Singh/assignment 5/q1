#include<bits/stdc++.h>
using namespace std;
int main(){
    int n,count=0,i;
    cin>>n;
    int a[n];
    for(i=0;i<n;i++)
    cin>>a[i];
    for(i=0;i<n;i++){
    if(a[i]==0){
    a[count++]=a[i];
    }
    }
    for(i=count;i<n;i++)
    a[i]=1;
    for(i=0;i<n;i++)
    cout<<a[i]<<" ";
    cout<<endl;
}
