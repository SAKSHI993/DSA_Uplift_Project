#include<bits/stdc++.h>
using namespace std;
int main(){
    int i,j,n;
    cin>>n;
    int a[n][n];
    for(i=1;i<=n;i++)
    {
        for(j=1;j<=n;j++)
        cin>>a[i][j];
    }
    for(i=1;i<=n;i++){
        cout<<a[i][i]<<" ";
    }
    cout<<endl;
    for(i=1;i<=n;i++){
        for(j=1;j<=n;j++){
        if(i+j==(n+1))
        cout<<a[i][j]<<" ";
        }
    }
    cout<<endl;
}
